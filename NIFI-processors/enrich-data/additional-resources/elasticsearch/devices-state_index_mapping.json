{
    "mappings":{
        "dynamic_templates":[
            {
                "numeric_values_long" : {
                    "path_match": "*.value" ,
                    "match_mapping_type": "long" ,
                    "mapping" : {
                        "type" : "float"
                    }
                }
            } ,
            {
                "numeric_values_double" : {
                    "path_match": "*.value" ,
                    "match_mapping_type": "double" ,
                    "mapping" : {
                        "type" : "float"
                    }
                }
            } ,
            {
                "string_values" : {
                    "path_match": "*.value_str" ,
                    "mapping" : {
                        "type": "keyword"
                    }
                }
            } ,
            {
                "obj_values": {
                    "path_match" : "*.value_obj" ,
                    "mapping" : {
                        "type" : "object"
                    }
                }
            } ,
            {
                "obj_values_str" : {
                    "path_match" : "*.value_obj" ,
                    "match_mapping_type" : "string" ,
                    "mapping" : {
                        "type" : "keyword"
                    }
                }
            } ,
            {
                "arr_obj" : {
                    "path_match" : "*.value_arr_obj" ,
                    "mapping" : {
                        "type" : "object"
                    }
                }
            } ,
            {
                "arr_obj_values_str" : {
                    "path_match": "*.value_arr_obj" ,
                    "match_mapping_type": "string" ,
                    "mapping": {
                        "type": "keyword"
                    }
                }
            }
        ] , 
        "properties" : {
            "date_time" : {
                "type": "date"
            } ,
            "latlon": { 
                "type": "geo_point"
            } ,
            "deviceModel" : {
            	"type": "keyword"
            } ,
            "deviceName" : {
            	"type": "keyword"
            }
        }
    }
}
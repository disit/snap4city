public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException, ParseException,SQLException {

if(first)
{
	first=false;		
	Connection conn=null;
	Statement statement=null;
	String selectTableSQL ="select * from ChargingStationObservation order by acquisitiontime DESC LIMIT 10";
	try {
		logBasic("Register Driver");
		Class.forName("org.apache.phoenix.jdbc.PhoenixDriver");
		logBasic("Creating connection");
		//conn=DriverManager.getConnection("jdbc:phoenix:127.0.0.1:2181:/hbase");
		conn = DriverManager.getConnection("jdbc:phoenix:192.168.0.118,192.168.0.124,192.168.0.129:2181:/hbase");
		logBasic("Creating statement");
		statement = conn.createStatement();
		ResultSet rs = statement.executeQuery(selectTableSQL);
		String serviceUri;
		String state;

		Writer writer = null;

		File file = new File("/media/Trasformazioni/Phoenix_ETL/snap4city/electric_vehicle_charging_datagate/Ingestion/", "ChargingStationObservation.csv");
        BufferedWriter bw = new BufferedWriter(new FileWriter(file.getAbsoluteFile()));
        bw.write("Header");

		//////--------
		String url ="http://192.168.0.206:8080/WebAppGrafo/api/v1/?serviceUri=http://www.disit.org/km4city/resource/eCharging_15EP22T2AA1S000001&format=json";
		URL obj = new URL(url);
		logBasic("\nSending 'GET' request to URL : " + url);
		HttpURLConnection con = (HttpURLConnection) obj.openConnection();
		con.setRequestMethod("GET");
		int responseCode = con.getResponseCode();
		logBasic("Response Code : " + responseCode);

		BufferedReader in = new BufferedReader(new InputStreamReader(con.getInputStream()));
		String inputLine;
		StringBuffer response = new StringBuffer();
		JSONParser parser = new JSONParser();

		while ((inputLine = in.readLine()) != null) {
			response.append(inputLine);
			JSONObject a = (JSONObject) parser.parse(inputLine);
		}
		in.close();
		logBasic(response.toString());
		//JSONObject objJSON = new JSONObject(response);
		logBasic("Vediamo se lo legge: "+a.get("services"));
//		JSONObject serviceJSON = objJSON.getJSONObject("Service");
//		JSONArray arr = serviceJSON.getJSONArray("features");
//		JSONObject geometryJSON = arr[0];
//		JSONArray coordinates = geometryJSON.getJSONArray("coordinates");
//		String latitude = coordinates[0];
//		String longitude = coordinates[1];
//		logBasic("Coordinatee: "+coordinates[0]+" "+coordinates[1]);
		///-------

		while (rs.next()) {
			serviceUri = rs.getString("SERVICEURI");
			state = rs.getString("CHARGINGSTATE");
 			bw.write("Received row: "+serviceUri+" state: "+state+"\n");
			logBasic("Received row: "+serviceUri+" state: "+state);
		}
        bw.close();
		conn.commit();
		logBasic("Connection closed! ");
		return true;
	}
	catch (Exception e)
	{
		logBasic("Generic Error: Apache or kettle -> that's the problem: "+e.toString());
	}
return true;
}

return true;
}
